<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane stylesheets="@../styles/styles.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.openjfx.controllers.MainController">
   <left>
      <VBox spacing="10.0" BorderPane.alignment="CENTER" alignment="TOP_CENTER" prefHeight="Infinity">
         <children>
            <Button fx:id="homeButton" mnemonicParsing="false" onAction="#showHome"/>
            <Button fx:id="importButton" mnemonicParsing="false" onAction="#showImport"/>
            <Button fx:id="exportButton" mnemonicParsing="false" onAction="#showExport"/>
            <Button fx:id="settingsButton" mnemonicParsing="false" onAction="#showSettings"/>
            <Button fx:id="aboutButton" mnemonicParsing="false" onAction="#showAbout"/>
            <Button fx:id="exitButton" mnemonicParsing="false" onAction="#exitApp"/>
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </VBox>
   </left>
   <center>
      <StackPane fx:id="contentPane" BorderPane.alignment="CENTER"/>
   </center>
</BorderPane>
